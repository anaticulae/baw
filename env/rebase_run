URL="${BASE}/${OWNER}/${REPO}.git"

echo $URL
git clone ${URL} -b ${BRANCH} upgrade && cd upgrade
echo $MAIN
git rebase $MAIN
echo $BRANCH
git push -u origin ${BRANCH}
